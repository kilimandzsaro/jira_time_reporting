<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Component</th>
      <th>Business</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.prefix %></td>
        <td><%= project.name %></td>
        <td><%= link_to 'Show', project %></td>
      </tr>
    <% end %>
  </tbody>
</table>


