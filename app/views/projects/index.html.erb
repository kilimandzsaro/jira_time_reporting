<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "Projects")%>
  <center><h1>Projects</h1></center>
  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td><%= link_to 'Show', project %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>