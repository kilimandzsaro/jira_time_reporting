<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "Report results")%>
  <center><h1>Report results</h1></center>
  <table>
    <thead>
      <tr>
        <th>Report name</th>
        <th>Run at</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @ran_reports.each do |report| %>
        <tr>
          <td><%= Report.find(report.report_id).name %>
          <td><%= ReportResult.order("updated_at DESC").find_by_report_id(report.report_id).updated_at %></td>
          <td><%= button_to 'Show', report_get_results_path(report.report_id), method: :get, class: 'btn btn-info btn-xs' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>