<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "New Report Type") %>
  <center><h1>New report types</h1></center>
  <%= form_for(@report_type, :url => url_for(:controller => 'report_types', :action => 'index')) do |f| %>
    <%= render 'shared/error_messages' %>
    <%= render 'report_types/form', f: f %>
    <%= f.submit "Save", class: "btn btn-large btn-primary" %>
  <% end %>
</div>

<%= link_to 'Back', report_types_path %>
