<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "Businesses")%>
  <center><h1>Businesses</h1></center>
  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @businesses.each do |business| %>
        <tr>
          <td><%= link_to 'Show', business %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
