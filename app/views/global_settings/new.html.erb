<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "New Settings") %>
  <center><h1>New Settings for reporting</h1></center>
  <%= form_for(@global_setting) do |f| %>
    <%= render 'shared/error_messages' %>
    <%= render 'form', f: f %>
    <%= f.submit "Save", class: "btn btn-large btn-primary" %>
  <% end %>
</div>

<%= link_to 'Back', global_settings_path %>
