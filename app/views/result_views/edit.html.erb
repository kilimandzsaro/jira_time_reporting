<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "Edit Template") %>
  <center><h1>Edit result view template</h1></center>
  <%= form_for @result_view do |f| %>
    <%= render 'shared/error_messages' %>
    <%= render 'form', f: f %>
    <%= f.submit "Save", class: "btn btn-large btn-primary" %>
  <% end %>
</div>

<%= link_to 'Back', result_views_path %>
