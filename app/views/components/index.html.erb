<div class="col-md-7 col-md-offset-3 main">
  <% provide(:title, "Components")%>
  <center><h1>Components</h1></center>
  <table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @components.each do |component| %>
        <tr>
          <td><%= component.id %></td>
          <td><%= component.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

