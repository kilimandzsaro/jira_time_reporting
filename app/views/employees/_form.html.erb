<%= f.fields_for :employees do |employee| %>
  <%= employee.text_field :name, value: @employee.name, :class => "form-control" %>
  <%= employee.text_field :email, value: @employee.email, :class => "form-control", :readonly => true %>
  <%= employee.text_field :status, value: @employee.status, :class => "form-control" %>
  <% employee.issue_history.each do |ih| %>
    <%= IssueHistory.find(ih).jira_name %>-<%= ih %>
  <% end %>
  <%= empolyee.collection_select :key, {1 => 'true', 2 => 'false'}, :key, :value, :class=> "form-control" %>
<% end %>